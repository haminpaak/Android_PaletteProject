<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:padding="20dp"
    tools:context=".SettingActivity">

    <LinearLayout
        android:id="@+id/Setting_LinearLayout_TOP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingTop="40dp"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/Setting_ImageButton_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00FF0000"
            android:contentDescription="@string/string_setting"
            app:srcCompat="@drawable/backbuttonicon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/nanumeb"
            android:text="@string/string_setting"
            android:textColor="#000000"
            android:textSize="28dp"
            tools:ignore="SpUsage" />

    </LinearLayout>

    <Button
        android:id="@+id/Setting_Button_MyProfile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:background="#00FF0000"
        android:fontFamily="@font/nanumr"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:stateListAnimator="@null"
        android:text="내 프로필"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Setting_LinearLayout_TOP"
        tools:ignore="SpUsage" />

    <Button
        android:id="@+id/Setting_Button_Developer"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="#00FF0000"
        android:fontFamily="@font/nanumr"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:stateListAnimator="@null"
        android:text="개발팀"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Setting_Button_MyProfile"
        tools:ignore="SpUsage" />

    <Button
        android:id="@+id/Setting_Button_Ask"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00FF0000"
        android:fontFamily="@font/nanumr"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:stateListAnimator="@null"
        android:text="@string/string_ask"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Setting_Button_Developer"
        tools:ignore="SpUsage" />

    <Button
        android:id="@+id/Setting_Button_Billing"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00FF0000"
        android:fontFamily="@font/nanumr"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:layout_marginTop="30dp"
        android:stateListAnimator="@null"
        android:text="구독 결제 관리"
        android:textAlignment="textStart"
        android:textColor="#2196F3"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Setting_Button_Ask"
        tools:ignore="SpUsage" />

    <Button
        android:id="@+id/Setting_Button_ChangePassword"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00FF0000"
        android:fontFamily="@font/nanumr"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:layout_marginTop="30dp"
        android:stateListAnimator="@null"
        android:text="비밀번호 변경"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Setting_Button_Billing"
        tools:ignore="SpUsage" />

    <Button
        android:id="@+id/Setting_Button_LogOut"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00FF0000"
        android:fontFamily="@font/nanumr"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:stateListAnimator="@null"
        android:text="로그아웃"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Setting_Button_ChangePassword"
        tools:ignore="SpUsage" />

    <Button
        android:id="@+id/Setting_Button_Quit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#00FF0000"
        android:fontFamily="@font/nanumr"
        android:paddingStart="10dp"
        android:paddingEnd="10dp"
        android:layout_marginTop="30dp"
        android:stateListAnimator="@null"
        android:text="@string/string_quit"
        android:textAlignment="textStart"
        android:textColor="#E50000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Setting_Button_LogOut"
        tools:ignore="SpUsage" />

</androidx.constraintlayout.widget.ConstraintLayout>